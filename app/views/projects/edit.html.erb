<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<%end %>

<%= link_to "Delete",
  project_path(@project),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%>
